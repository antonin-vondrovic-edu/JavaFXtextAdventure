<VBox prefHeight="720.0" prefWidth="1175.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.educanet.gui.MenuController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Quit" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefWidth="897.0" style="-fx-background-color: #1c1c1c;" VBox.vgrow="ALWAYS">
            <children>
            <Text fill="#d33333" layoutX="267.0" layoutY="282.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sorry but, there are no options :/" textAlignment="CENTER" wrappingWidth="641.13671875">
               <font>
                  <Font name="Times New Roman" size="96.0" />
               </font>
            </Text>
            <Text fill="WHITE" layoutX="267.0" layoutY="460.0" strokeType="OUTSIDE" strokeWidth="0.0" text="i just wanted an options button, it looked cool :D" textAlignment="CENTER" wrappingWidth="641.13671875">
               <font>
                  <Font size="19.0" />
               </font>
            </Text>
            <Button fx:id="buttonOptions" layoutX="390.0" layoutY="521.0" mnemonicParsing="false" onAction="#returnToMenu" prefHeight="52.0" prefWidth="395.0" text="Return to Main Menu">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            </children>
        </AnchorPane>
    </children>
</VBox>
